@using AutoMapper.Internal
@model TH.WebUI.ViewModels.RequireCredentialDetailsViewModel
@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<article>
    <h2>@Model.Title</h2>
    <h3>发布于 @Model.CreateDate</h3>
    <section>
        <h3>基本信息</h3>
        <ul>
            <li>所持证件类型: @Model.Type</li>
            <li>专业: @Model.Major</li>
            <li>证件类别: @Model.Class</li>
            <li>价格: @Model.Price</li>
        </ul>
        <h3>联系方式</h3>
        <ul>
            <li>联系人: @Model.ContactPerson</li>
            <li>联系电话: @Model.Telephones</li>
        </ul>
    </section>
</article>

